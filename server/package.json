{
  "name": "shepherd-server",
  "version": "0.0.1",
  "description": "Integrate and serve data for shepherd",
  "private": false,
  "dependencies": {
    "autoprefixer-core": "^3.1.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.8.1",
    "cookie-parser": "^1.3.2",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "docker-registry-client": "^3.2.12",
    "express": "^4.10.4",
    "express-session": "^1.9.2",
    "jade": "^1.7.0",
    "lodash": "^4.17.11",
    "parallelshell": "^2.0.0",
    "pg": "^6.1.0",
    "socket.io": "^1.5.1",
    "superagent": "~4.0.0-beta.5"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "nodemon": "^1.12.1",
    "should": "^13.2.3",
    "socket.io-client": "^2.1.1",
    "@types/chai": "^4.1.6",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "@types/sinon": "^5.0.5",
    "@types/supertest": "^2.0.6",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "typescript-formatter": "^7.2.2"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "ci": "npm run build && npm test",
    "clean": "rm -rf .dist",
    "compile": "tsc",
    "e2etdd-local": "SERVICE_SUFFIX= mocha --exit -R dot --require ts-node/register --recursive --watch-extensions ts,js --watch './src/**/*.e2e.ts'",
    "e2etest": "SERVICE_SUFFIX=.dev.it.tm.is mocha --exit --require ts-node/register --recursive './src/**/*.e2e.ts'",
    "contract-test": "SERVICE_SUFFIX=.dev.it.tm.is mocha -R dot --exit --require ts-node/register --recursive './src/**/*.contract-spec.ts'",
    "contract-tdd": "SERVICE_SUFFIX=.dev.it.tm.is mocha --exit --require ts-node/register --recursive  --watch-extensions ts,js --watch  './src/**/*.contract-spec.ts'",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "format-verify": "prettier -l 'src/**/*.{ts,js}'",
    "precommit": "echo 'Precommit' && npm run build && npm run format-verify && ../repo_scripts/detect_only.sh",
    "prepush": "npm run ci",
    "tdd": "mocha -R dot --require ts-node/register --recursive --watch-extensions ts,js --watch './src/**/*.spec.ts'",
    "test": "mocha --exit --require ts-node/register --recursive './src/**/*.spec.ts'"
  }
}
