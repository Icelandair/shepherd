{
  "name": "nano-inject",
  "version": "1.0.0",
  "description": "Smallest dependency injection framework imaginable",
  "keywords": [
    "redis",
    "cache",
    "promise"
  ],
  "author": "Gu√∞laugur S. Egilsson <gulli@kolibri.is>",
  "homepage": "",
  "license": "ISC",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "ci": "npm run clean && npm run test && npm run build && npm run format-verify",
    "e2etest": "mocha --exit --require ts-node/register --recursive ./**/*.e2e.ts",
    "e2etdd": "mocha --exit -R dot --require ts-node/register --recursive --watch-extensions ts,js --watch ./**/*.e2e.ts",
    "test": "mocha --exit --require ts-node/register --recursive ./**/*.spec.ts",
    "tdd": "mocha -R dot --require ts-node/register --recursive --watch-extensions ts,js --watch ./**/*.spec.ts",
    "prepush": "npm run ci",
    "format": "prettier --ignore-path ../../.prettierignore --write 'lib/**/*.{ts,js}'",
    "format-verify": "prettier  --ignore-path ../../.prettierignore -l 'lib/**/*.{ts,js}'"
  },
  "dependencies": {},
  "devDependencies": {
  }
}
